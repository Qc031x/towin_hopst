<script src="js/leftside.js" type="text/javascript"></script>

<script>
    var setting = {
        view: {
            selectMulti: true,
            dblClickExpand: false,
        },
        data: {
            simpleData: {
                enable: true
            }
        },
        callback: {
            onClick: showInfo
        }
    },

    city = [
        {id:1, pId:0, name: "城市", open:true},
        {id:2, pId:1, name: "广州市", open:true},
        {id:3, pId:2, name: "天河分院"},
        {id:4, pId:2, name: "荔湾分院"},
        {id:5, pId:1, name: "深圳市", open:true},
        {id:50, pId:5, name: "南山分院", esid:'nanshan'},
        {id:51, pId:5, name: "福田分院", esid:'futian'}

    ], ztreeObj;

    $(document).ready(function() {

        ztreeObj = $.fn.zTree.init($("#entity"), setting, city);
        ztreeObj.expandAll(true);

        $(".leftul span").on("click", function() {
            $(this).addClass("active").siblings().removeClass("active");
            $("#" + $(this).attr("data-source")).show().siblings(".ztree").hide();
        })
        //右侧边栏预约主体切换
        $('.cek').click(function() {
            $(this).parent().parent().addClass('trActive');
            var format = $(this).parent().siblings(".cng").html();
            $(".blFormat").html(format);
            $('.checkdetail').css('display', 'block');

        });

        //右侧边栏底部操作
        $('.undo').click(function() {
            $('.checkdetail').css('display', 'none');
        });

        //数据超过一页显示分页
        if($('tbody tr').size() > 8) {
            $(".tcdPageCode").css('display', 'block');
        } else {
            $(".tcdPageCode").css('display', 'none');
        }


    });

    function showInfo(event, treeId, treeNode, esid) {
        var id = treeNode.esid;
        $('#'+id).find('.hds').text(treeNode.name);
        $('#'+id).show().siblings().hide();
    };
</script>
