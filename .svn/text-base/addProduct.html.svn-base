    function verifyNum(num,id){
        if(!isNaN(num)){
            if(Number(num)==0){
                $("#"+id).val("");
            }
        }else{
            $("#"+id).val("");
        }
    }

    function openFile() {
        $("#fileName ").click();
    }
</script>
</body>
</html>

