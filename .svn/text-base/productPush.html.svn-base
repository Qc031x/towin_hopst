        function initUI(pageNo, pageTotal, pageSize, objId) {
            pagination({
                cur: pageNo,
                total: pageTotal,
                len: 5,
                targetId: objId,
                callback: function () {
                    var oPages = document.getElementsByClassName('page-index');
                    for (var i = 0; i < oPages.length; i++) {
                        oPages[i].onclick = function () {
                            initUI(this.getAttribute('data-index'), pageTotal, 3, objId);
                        }
                    }
                    $('.firstPage').click(function () {
                        initUI(1, pageTotal, 3, objId);
                    });
                    $('.lastPage').click(function () {
                        initUI(total, pageTotal, 3, objId);
